@model AdmCRRN.Models.Entidade

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<fieldset>
    <legend>Entidade</legend>

    <div class="display-label">Cnpj</div>
    <div class="display-field">@Model.Cnpj</div>

    <div class="display-label">Nome</div>
    <div class="display-field">@Model.Nome</div>

    @Html.Partial("~/Views/Endereco/_Details.cshtml", Model)

</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>

